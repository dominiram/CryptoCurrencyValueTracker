<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="general_info_text_view">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/cool_gray</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="y_coordinate_text_view">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/design_default_color_primary</item>
    </style>

    <style name="graph_view_time_to_compare_buttons">
        <item name="android:layout_width">55dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/cool_gray</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/recycler_view_blue</item>
    </style>
</resources>